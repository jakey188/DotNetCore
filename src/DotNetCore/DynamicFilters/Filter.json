/* `table`.`id` = 1 AND (`table`.`id` = 2 OR `table`.`id` = 3)*/
{
  "Concat": "And",
  "Filters": [ /*条件连接符And,Or。注:Table/Field/Operator/FieldValue/FieldType 与 Concat/Filters 不要同时设置*/
    {
      "Table": "table", /*表名*/
      "Field": "id", /*字段名*/
      "Operator": "Eq", /*操作符[Eq,Ne,Lt,Gt,Le,Ge,In,Contains]*/
      "FieldValue": "1", /*字段值[多个值用,分割]*/
      "FieldType": "Number" /*字段类型[Number,String,Boolean,DateTime]*/
    },
    {
      "Concat": "Or",
      "Filters": [
        {
          "Table": "table",
          "Field": "id",
          "Operator": "Eq",
          "FieldValue": "1",
          "FieldType": "Number"
        },
        {
          "Table": "table",
          "Field": "id",
          "Operator": "Eq",
          "FieldValue": "1",
          "FieldType": "Number"
        }
      ]
    }
  ]
}