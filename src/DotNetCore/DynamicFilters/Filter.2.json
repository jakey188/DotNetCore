/* id = 1 AND (id = 2 OR id = 3)*/
{
  "Concat": "And",
  "Filters": [ /*条件连接符And,Or。注:Field/Operator/Value 与 Concat/Filters 不要同时设置*/
    {
      "Field": "id", /*字段名*/
      "Operator": "Eq",
      "Value": 1
    },
    {
      "Concat": "Or",
      "Filters": [
        {
          "Field": "id",
          "Operator": "Eq",
          "Value": 2
        },
        {
          "Field": "id",
          "Operator": "Eq",
          "Value": 3
        }
      ]
    }
  ]
}